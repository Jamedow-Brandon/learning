<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jamedow.learning.dao.UserDao">

    <insert id="insertUser" parameterType="com.jamedow.learning.dmo.User" keyProperty="id">
        insert into users(
        username,
        password)
        values
        (
        #{username},
        #{password})
    </insert>

</mapper>